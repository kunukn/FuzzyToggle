{
  "name": "@kunukn/fuzzytoggle",
  "version": "0.0.5",
  "description": "Fuzzy toggle",
  "main": "dist/FuzzyToggle.umd.js",
  "main-es2015": "dist/FuzzyToggle.umd.es2015.js",
  "module": "dist/FuzzyToggle.es.js",
  "cjs": "dist/FuzzyToggle.cjs.js",
  "iife": "dist/FuzzyToggle.iife.js",
  "jsnext:main": "dist/FuzzyToggle.es.js",
  "scripts": {
    "clean": "rimraf dist/*.*",
    "start": "webpack-dev-server --color --mode development --env.PORT=6007",
    "validate": "webpack-dev-server --color --mode development --env.VALIDATE=1 --env.PORT=6008",
    "v": "yarn validate",
    "build-es5": "rollup -c rollup.config.js --environment ES5",
    "build-es6": "rollup -c rollup.config.js --environment ES6",
    "build": "yarn clean && yarn build-es5 && yarn build-es6",
    "b": "yarn build-es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunukn/FuzzyToggle.git"
  },
  "keywords": [
    "library"
  ],
  "files": [
    "dist"
  ],
  "browserslist": [],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.16.0",
